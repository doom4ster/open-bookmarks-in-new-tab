{
  "manifest_version": 3,
  "name": "Open Bookmarks in New Tab (Force)",
  "version": "1.0.2",
  "description": "由書籤觸發導覽時，自動在新分頁開啟，並避免資料夾/分頁群組批次開啟時的重複。",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "Open Bookmarks in New Tab (Force)"
  },
  "homepage_url": "https://github.com/doom4ster/open-bookmarks-in-new-tab" 
}
